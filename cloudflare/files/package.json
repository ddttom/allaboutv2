{
  "name": "allabout-cloudflare-worker",
  "version": "1.0.0",
  "description": "Custom Adobe EDS Cloudflare Worker with CORS and JSON-LD generation",
  "main": "cloudflare-worker.js",
  "scripts": {
    "test": "vitest run cloudflare-worker.test.js",
    "test:integration": "vitest run cloudflare-worker.integration.test.js",
    "test:all": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:server": "./test-server.sh",
    "test:manual": "./manual-test.sh",
    "compare": "./compare-responses.sh",
    "compare:visual": "open compare-visual.html",
    "dev": "wrangler dev cloudflare-worker.js",
    "deploy": "wrangler deploy cloudflare-worker.js",
    "tail": "wrangler tail",
    "lint": "eslint cloudflare-worker.js"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "adobe",
    "eds",
    "cors",
    "json-ld",
    "seo"
  ],
  "author": "Digital Domain Technologies Ltd",
  "license": "Apache-2.0",
  "devDependencies": {
    "wrangler": "^3.80.0",
    "vitest": "^2.1.0",
    "@vitest/coverage-v8": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
